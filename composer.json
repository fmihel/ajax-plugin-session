{
    "name": "fmihel/ajax-plugin-session",
    "description": "php,session,plugin, ajax",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "fmihel",
            "email": "fmihel76@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "fmihel\\": "server"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "fmihel\\test\\": "tests"
        }
    },
    "require": {
        "fmihel/ajax": ">=0.0.7",
        "smoren/mushroom-hook-manager": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "smoren/mushroom-hook-manager": true
        }
    },
    "extra": {
        "mushroom-use-hooks": true,
        "mushroom-hooks": {
            "after-install": [
                "fmihel\\MushroomHooks::afterInstall"
            ],
            "after-update": [
                "fmihel\\MushroomHooks::afterInstall"
            ]
        },
        "mushroom-clear":[
            "client", ".npmignore","package.json","package-lock.json","rollup.config.js","rollup.config.min.js",".gitignore"
        ]
            
        
    }        
}
